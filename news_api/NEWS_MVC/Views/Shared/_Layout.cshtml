
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <title>@ViewData["Title"] - Zero Online-International</title>
    <meta name="keywords" content="War of Robots, Zero Online-International" />
    @{
        var desc = ViewData["Description"] as string;
        if (!string.IsNullOrEmpty(desc))
        {
            <meta name="description" content="@desc" />
        }
        else
        {
            <meta name="description" content="The 2.5D 'Machine Warfare' online game is developed by Tianqing Digital for many years and collects the wisdom of countless online game players. The game is based on the mechanical theme, perfectly depicts a very contemporary futuristic world and penetrates such elements as a high-tech war between planets caused by resources and territories. main level of play. The latest expansion Zerg Havoc on May 17, 2012 perfectly presented the grand event of the alien invasion." />
        }
    }
    <meta http-equiv="x-ua-compatible" content="ie=7" />
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8">
    @*<meta property="fb:app_id" content="946692825507723" />*@
    <meta property="og:image:width" content="600">
    <meta property="og:image:height" content="400">
    <meta property="og:url" content="@ViewBag.Domain">
    <meta property="og:type" content="website">
    @{
        var title = @ViewData["Title"] as string;
        if (!string.IsNullOrEmpty(title))
        {
            <meta property="og:title" content="@title">
        }
        else
        {
            <meta property="og:title" content="War of Robots, Zero Online-International">
        }
    }
    @{
        if (!string.IsNullOrEmpty(desc))
        {
            <meta property="og:description" content="@desc">
        }
        else
        {
            <meta property="og:description" content="Open new Gemini server, join now to experience and receive exciting parts every day">
        }
    }
    @{
        var thumbnail = ViewData["Thumbnail"] as string;
        if (!string.IsNullOrEmpty(thumbnail))
        {
            <meta property="og:image" content="@thumbnail">
        }
        else
        {
            <meta property="og:image" content="@ViewBag.Domain/images/sub-banner.png?v=@NEWS_MVC.Helpers.AssemblyHelper.GetLastModify()">
        }
    }
    <meta property="og:site_name" content="@ViewBag.Domain">
   
    <script type="application/ld+json">
        {
          "@@context": "https://schema.org",
          "@@type": "BreadcrumbList",
          "itemListElement": [{
            "@@type": "ListItem",
            "position": 1,
            "name": "Zero Online-International",
            "item": "@ViewBag.Domain"
          },{
            "@@type": "ListItem",
            "position": 2,
            "name": "@ViewBag.Domain ✅ (Đã Xác Minh)",
            "item": "@ViewBag.Domain"
          }]
        }
    </script>



    <link rel="stylesheet" href="~/css/idangerous.swiper.min.css" asp-append-version="true" />
    <link href="https://fonts.googleapis.com/css2?family=Jura:wght@300;400;500;600;700&display=swap" async rel="stylesheet" />
    <style>
        body {
            font-family: 'Jura', sans-serif;
            margin: 0;
        }
    </style>
    <script async="" src="https://www.googletagmanager.com/gtag/js?id=G-C9J35JMGF8"></script>
    <script>
        window.dataLayer = window.dataLayer || [];
        function gtag() { dataLayer.push(arguments); }
        gtag('js', new Date());

        gtag('config', 'G-C9J35JMGF8');
    </script>
</head>
<body>
    @RenderBody()
    @{
        var renderScripts = ViewData["RenderScript"] as string ?? "1";
        if (renderScripts == "1")
        {
            if (Env.IsDevelopment())
            {
                <script src="~/js/g_topmenu.js" asp-append-version="true"></script>
                <script src="~/js/public.js" asp-append-version="true"></script>
                <script src="~/js/jquery-1.8.3.min.js" asp-append-version="true"></script>
                <script src="~/js/idangerous.swiper2.7.6.min.js" asp-append-version="true"></script>
                <script src="~/js/custom.js" asp-append-version="true"></script>
            }
            else
            {
                <script src="~/js/g_topmenu.min.js" asp-append-version="true"></script>
                <script src="~/js/public.min.js" asp-append-version="true"></script>
                <script src="~/js/jquery-1.8.3.min.js" asp-append-version="true"></script>
                <script src="~/js/idangerous.swiper2.7.6.min.js" asp-append-version="true"></script>
                <script src="~/js/custom.min.js" asp-append-version="true"></script>
            }
        }
    }

    @RenderSection("Scripts", required: false)
</body>
</html>
